#tag Events RoundPopup1
	#tag Event
		Sub Open()
		  dim fCount as integer = fontCount - 1
		  dim i as integer
		  
		  for i = 0 to fCount
		    me.addRow font(i)
		  next
		  
		  me.listIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Change()
		  EditField2.text = EditField2.text + "You selected """ + me.text+ """ from Font menu" +endOfLine
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events RoundPopup2
	#tag Event
