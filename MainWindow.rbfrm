		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events RoundPopup3
	#tag Event
		Sub Open()
		  me.menu.Items(0).Style = "bold"
		  me.menu.Items(1).Style = "Italic"
		  me.menu.Items(2).Style = "Underline"
		End Sub
	#tag EndEvent
	#tag Event
		Sub Change()
